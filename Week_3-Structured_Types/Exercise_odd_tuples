def oddTuples(aTup):
    '''
    aTup: a tuple
    
    returns: tuple, every other element of aTup. 
    '''
    # Your Code Here
    ret = ()
    x = 1
    for i in aTup:
        
        if(x%2!=0):
            ret = ret+(i,)
    
        x+=1
    return ret
    
    
    
a = ()

aTup = (1,2,3,4,5)
a = oddTuples(aTup)    

print("",a)
